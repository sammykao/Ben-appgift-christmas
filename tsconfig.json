{
  "extends": "./node_modules/expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@app/*": [
        "app/*"
      ],
      "@src/*": [
        "src/*"
      ]
    }
  },
  "include": [
    "app",
    "src",
    "expo-env.d.ts",
    "env.d.ts",
    ".expo/types/**/*.ts"
  ]
}
